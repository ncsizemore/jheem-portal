/**
 * JHEEM Logo Design Options
 * Hexsticker-style logos for academic use
 *
 * Usage: Import the specific logo variant you want to use
 */

interface LogoProps {
  className?: string;
  size?: number;
}

// Option A: Epidemiological Network
// Network nodes with connections representing HIV transmission dynamics
export function JheemLogoNetwork({ className = "", size = 200 }: LogoProps) {
  const height = size * 1.15; // Hex proportions

  return (
    <svg
      viewBox="0 0 200 230"
      className={className}
      style={{ width: size, height }}
    >
      {/* Hexagon border */}
      <path
        d="M 100 10 L 180 60 L 180 160 L 100 210 L 20 160 L 20 60 Z"
        fill="white"
        stroke="#002D72"
        strokeWidth="4"
      />

      {/* Network nodes (representing populations/states) */}
      <circle cx="70" cy="70" r="11" fill="#002D72" opacity="0.3" />
      <circle cx="130" cy="70" r="11" fill="#002D72" opacity="0.3" />
      <circle cx="100" cy="95" r="14" fill="#002D72" opacity="0.8" />
      <circle cx="65" cy="120" r="9" fill="#002D72" opacity="0.3" />
      <circle cx="135" cy="120" r="9" fill="#002D72" opacity="0.3" />
      <circle cx="100" cy="140" r="11" fill="#002D72" opacity="0.3" />

      {/* Connection lines (transmission pathways) */}
      <line x1="70" y1="70" x2="100" y2="95" stroke="#002D72" strokeWidth="2" opacity="0.3" />
      <line x1="130" y1="70" x2="100" y2="95" stroke="#002D72" strokeWidth="2" opacity="0.3" />
      <line x1="100" y1="95" x2="65" y2="120" stroke="#002D72" strokeWidth="2" opacity="0.3" />
      <line x1="100" y1="95" x2="135" y2="120" stroke="#002D72" strokeWidth="2" opacity="0.3" />
      <line x1="100" y1="95" x2="100" y2="140" stroke="#002D72" strokeWidth="2" opacity="0.3" />
      <line x1="65" y1="120" x2="100" y2="140" stroke="#002D72" strokeWidth="2" opacity="0.2" />
      <line x1="135" y1="120" x2="100" y2="140" stroke="#002D72" strokeWidth="2" opacity="0.2" />

      {/* JHEEM text - fits within hex bounds */}
      <text
        x="100"
        y="180"
        textAnchor="middle"
        fontFamily="system-ui, -apple-system, sans-serif"
        fontSize="28"
